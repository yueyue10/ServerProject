[
  {
    "author": "骆宾王",
    "content": [
      "鹅，鹅，鹅，曲项向天歌。",
      "白毛浮绿水，红掌拨清波。"
    ],
    "time": "[唐]",
    "title": "《咏鹅》"
  },
  {
    "author": "汉乐府",
    "content": [
      "江南可采莲，",
      "莲叶何田田。",
      "鱼戏莲叶间。",
      "鱼戏莲叶东，",
      "鱼戏莲叶西，",
      "鱼戏莲叶南，",
      "鱼戏莲叶北。"
    ],
    "time": "[汉]",
    "title": "《江南》"
  }
]
